{
  "name": "quordle",
  "version": "1.1.1",
  "description": "Put your skills to the test and solve four Wordles at once! You have 9 guesses to solve all four words. A new Quordle available each day to solve.",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "deploy-prod": "env-cmd -f ./.env vite build && echo www.quordle.com > dist/CNAME && gh-pages --dist dist --repo https://github.com/fireph/quordle.git --branch gh-pages --src **/!(*.js.map)"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/gtag.js": "^0.0.8",
    "@types/mersenne-twister": "^1.1.2",
    "@types/node": "^17.0.18",
    "autoprefixer": "^10.4.2",
    "babel-preset-solid": "^1.3.6",
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.5.5",
    "vite": "^2.8.3",
    "vite-plugin-ejs": "^1.4.3",
    "vite-plugin-sentry": "^1.0.13",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "file-saver": "^2.0.5",
    "mersenne-twister": "^1.1.0",
    "solid-app-router": "^0.2.1",
    "solid-dismiss": "^1.0.27",
    "solid-js": "^1.3.7"
  }
